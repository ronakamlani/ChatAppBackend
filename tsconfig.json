{
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "pretty": true,
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "rootDirs": [
      "./src"
    ],
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "esModuleInterop": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "typeRoots":[
      "src/types/types.express.d.ts",
      "node_modules/@types"
    ],
    "paths": {
      "*": [
          "node_modules/*",
          "src/types/*"
      ]
    }
  },
  "include": ["src/**/**/*"],
  "exclude": [
    "node_modules",
    "src/**/*.spec.ts"
  ],
  "ts-node": {
    "files": true
  },
  "files":[
    "src/types/types.express.d.ts"
  ]
}